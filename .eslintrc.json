{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
        "no-duplicate-imports": "warn",
        "arrow-body-style": ["error", "always"]
    }
}
